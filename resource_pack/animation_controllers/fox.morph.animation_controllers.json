{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.morph.fox.move" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"blend_transition" : 0.1,
					"transitions" : [
						{
							"sleep" : "query.is_sleeping"
						},
						{
							"wiggle" : "query.is_sneaking && query.is_on_ground && !query.is_in_water"
						}
					]
				},
				"pounce" : {
					"animations" : [ "fox_pounce" ],
					"transitions": [
						{
							"default" : "query.is_on_ground || query.is_in_water"
						}
					]
				},
				"sleep" : {
					"animations" : [ "fox_sleep" ],
					"blend_transition" : 0.2,
					"transitions" : [
						{
							"default" : "!query.is_sleeping"
						}
					]
				},
				"wiggle" : {
					"animations" : [ "fox_wiggle" ],
					"blend_transition" : 0.2,
					"transitions" : [
						{
							"default": "!query.is_sneaking || query.is_in_water"
						},
						{
							"pounce" : "!query.is_on_ground"
						}
					]
				}
			}
		}
	}
}
