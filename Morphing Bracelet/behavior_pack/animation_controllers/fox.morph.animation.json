{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.morph.fox.attack": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            "fox.attack.stop"
          ],
          "transitions": [
            {
              "attack": "query.is_sneaking && query.is_on_ground && query.property('morph:entity') == 12"
            }
          ]
        },
        "attack": {
          "animations": [
            "fox.attack"
          ],
          "transitions": [
            {
              "default": "!query.is_sneaking || !query.is_on_ground || query.property('morph:entity') != 12"
            }
          ]
        }
      }
    }
  }
}