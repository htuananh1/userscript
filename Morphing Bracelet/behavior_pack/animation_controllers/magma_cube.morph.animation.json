{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.morph.magma_cube.jumping": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "on_ground": "query.is_on_ground && query.property('morph:entity') == 53"
            },
            {
              "on_air": "!query.is_on_ground && query.property('morph:entity') == 53"
            }
          ]
        },
        "on_ground": {
          "transitions": [
            {
              "big_jump": "query.is_jumping && query.variant > 1"
            },
            {
              "small_jump": "query.is_jumping && query.variant <= 1"
            },
            {
              "on_air": "!query.is_on_ground"
            },
            {
              "default": "query.property('morph:entity') != 53"
            }
          ]
        },
        "on_air": {
          "transitions": [
            {
              "big_land": "!query.is_jumping && query.is_on_ground && query.variant > 1"
            },
            {
              "small_land": "!query.is_jumping && query.is_on_ground && query.variant <= 1"
            },
            {
              "on_ground": "query.is_jumping && query.is_on_ground"
            },
            {
              "default": "query.property('morph:entity') != 53"
            }
          ]
        },
        "big_jump": {
          "on_entry": [
            "/playsound mob.magmacube.big @a ~ ~ ~ 1.0 0.80"
          ],
          "transitions": [
            {
              "on_air": "!query.is_on_ground"
            },
            {
              "default": "query.property('morph:entity') != 53"
            }
          ]
        },
        "small_jump": {
          "on_entry": [
            "/playsound mob.magmacube.small @a ~ ~ ~ 1.0 0.80"
          ],
          "transitions": [
            {
              "on_air": "!query.is_on_ground"
            },
            {
              "default": "query.property('morph:entity') != 53"
            }
          ]
        },
        "big_land": {
          "on_entry": [
            "/playsound mob.magmacube.big @a ~ ~ ~ 1.0 0.80"
          ],
          "transitions": [
            {
              "on_ground": "query.is_on_ground"
            },
            {
              "default": "query.property('morph:entity') != 53"
            }
          ]
        },
        "small_land": {
          "on_entry": [
            "/playsound mob.magmacube.small @a ~ ~ ~ 1.0 0.80"
          ],
          "transitions": [
            {
              "on_ground": "query.is_on_ground"
            },
            {
              "default": "query.property('morph:entity') != 53"
            }
          ]
        }
      }
    }
  }
}