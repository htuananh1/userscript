{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.morph.goat.long_jump": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            "goat.long_jump.stop"
          ],
          "transitions": [
            {
              "long_jump": "query.is_sneaking && query.is_on_ground && query.property('morph:entity') == 36"
            }
          ]
        },
        "long_jump": {
          "animations": [
            "goat.long_jump"
          ],
          "transitions": [
            {
              "jumped": "query.is_jumping"
            },
            {
              "default": "!query.is_sneaking || !query.is_on_ground || query.property('morph:entity') != 36"
            }
          ]
        },
        "jumped": {
          "on_entry": [
            "/playsound component.jump_to_block @a ~ ~ ~"
          ],
          "transitions": [
            {
              "default": "!query.is_sneaking || !query.is_on_ground || query.property('morph:entity') != 36"
            }
          ]
        }
      }
    }
  }
}