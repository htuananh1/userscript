		},
		"animation.morph.pufferfish.swim" : {
			"loop" : true,
			"bones" : {
				"tailfin" : {
					"rotation" : [ 0.0, "math.cos((query.time_stamp + query.frame_alpha) * 30.0) * -5.75", 0.0 ]
				}
			}
		},
		"animation.morph.pufferfish.look_at_target": {
			"loop": true,
			"bones": {
